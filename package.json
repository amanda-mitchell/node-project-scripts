{
  "name": "@amanda-mitchell/node-project-scripts",
  "version": "1.0.0",
  "description": "Common scripts and configuration for my node projects.",
  "main": "index.js",
  "repository": "https://github.com/amanda-mitchell/node-project-scripts",
  "author": "Amanda Mitchell",
  "license": "MIT",
  "private": false,
  "files": [
    "*.js",
    "scripts/*.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "_install": "node scripts/install.js",
    "prepublishOnly": "pinst --enable",
    "postpublish": "pinst --disable"
  },
  "dependencies": {
    "@amanda-mitchell/semantic-release-notify-dependabot": "^1.0.0",
    "@amanda-mitchell/semantic-release-npm-multiple": "^2.1.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "eslint": "^7.13.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "pkg-dir": "^5.0.0",
    "prettier": "2.1.2",
    "semantic-release": "^17.2.2"
  },
  "devDependencies": {
    "pinst": "^2.0.0"
  }
}

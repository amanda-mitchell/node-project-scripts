{
  "name": "@amanda-mitchell/node-project-scripts",
  "version": "1.0.0",
  "description": "Common scripts and configuration for my node projects.",
  "main": "index.js",
  "repository": "https://github.com/amanda-mitchell/node-project-scripts",
  "author": "Amanda Mitchell",
  "license": "MIT",
  "private": false,
  "files": [
    "*.cjs",
    "scripts/*.mjs",
    "tsconfig.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "_install": "node scripts/install.mjs",
    "prepublishOnly": "pinst --enable",
    "postpublish": "pinst --disable"
  },
  "dependencies": {
    "@amanda-mitchell/semantic-release-npm-multiple": "^2.1.1",
    "@commitlint/cli": "^16.0.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@tsconfig/node12": "^1.0.7",
    "@types/jest": "^27.0.0",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "husky": "4.x",
    "jest": "^27.5.1",
    "lint-staged": "^12.0.2",
    "pkg-dir": "^6.0.0",
    "prettier": "2.5.1",
    "semantic-release": "^19.0.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "pinst": "^3.0.0"
  }
}
